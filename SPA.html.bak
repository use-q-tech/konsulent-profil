    <script>
        document.addEventListener('DOMContentLoaded', () => {
            function router() {
                // Get hash or default to 'overblik'
                let hash = window.location.hash.replace('#', '');
                if (!hash) hash = 'overblik';

                const routes = ['overblik', 'pitch', 'cv'];
                if (!routes.includes(hash)) hash = 'overblik';

                document.querySelectorAll('.view-section').forEach(el => {
                    el.classList.add('hidden');
                    el.classList.remove('active');
                });

                const activeView = document.getElementById('view-' + hash);
                if (activeView) {
                    activeView.classList.remove('hidden');
                    activeView.classList.add('active');
                    window.scrollTo(0, 0);
                }

                // Update Navbar Links
                document.querySelectorAll('.nav-link').forEach(link => {
                    const linkHash = link.getAttribute('href').replace('#', '');
                    
                    if (linkHash === hash) {
                        link.classList.add('text-primary', 'font-semibold', 'bg-slate-100');
                        link.classList.remove('text-text-muted', 'font-medium', 'hover:text-primary');
                    } else {
                        link.classList.remove('text-primary', 'font-semibold', 'bg-slate-100');
                        link.classList.add('text-text-muted', 'font-medium', 'hover:text-primary');
                    }
                });

                const cvBtn = document.getElementById('nav-btn-cv');
                if(cvBtn) {
                    if(hash === 'cv') {
                        cvBtn.classList.add('bg-primary', 'text-white', 'shadow-md');
                        cvBtn.classList.remove('bg-white', 'text-text-main', 'border', 'border-border-subtle');
                    } else {
                        cvBtn.classList.remove('bg-primary', 'text-white', 'shadow-md');
                        cvBtn.classList.add('bg-white', 'text-text-main', 'border', 'border-border-subtle');
                    }
                }
            }

            window.addEventListener('hashchange', router);
            router(); 
        });
    </script>
